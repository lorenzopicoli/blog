      "name": "rainfall",
      "transform": [
        {
          "type": "formula",
          "expr": "datetime(datum.date)",
          "as": "date"
        },
        {
          "type": "formula",
          "expr": "dayofyear(datum.date)",
          "as": "dayOfYear"
        },
        {
          "type": "formula",
          "expr": "month(datum.date)",
          "as": "month"
        },
        {
          "type": "formula",
          "expr": "2 * PI * (datum.dayOfYear / 365) - PI/2",
          "as": "angle"
        },
        {
          "type": "formula",
          "expr": "(datum.dayOfYear / 365) * (radius - 30) + 20",
          "as": "radius"
        },
        {
          "type": "formula",
          "expr": "datum.radius * cos(datum.angle)",
          "as": "x"
        },
        {
          "type": "formula",
          "expr": "datum.radius * sin(datum.angle)",
          "as": "y"
        }
      ]
    },
